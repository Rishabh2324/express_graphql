{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended","plugin:prettier/recommended"],
    "plugins": [
        "prettier"
    ],

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"warn",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true, 
                "endOfline": "auto"
            }
        ]    
    }
}
